[user]
	name = zackad
	email = zackad.dev@gmail.com
	signingKey = 5E11F0B6C1EF4843
[alias]
	q = log --all --decorate --oneline --graph
	l = log --date=short
	cm = checkout master
[format]
	pretty = format:%C(yellow)%ad %Cblue%h %Cgreen%<(7)%aN:%Cred%d %Creset%s
[log]
	date = format:%Y-%m-%d %H:%M:%S
[core]
	pager = delta
	excludesFile = ~/.gitignore
[merge]
	ff = no
[delta]
	line-numbers = true
    hunk-header-style = omit
[tag]
	gpgSign = true
[init]
	defaultBranch = master

[user]
	name = zackad
	email = zackad.dev@gmail.com
	signingkey = D9944489
[core]
	autocrlf = input
	pager = delta
	editor = vim
[push]
	default = matching
[color]
	ui = true
[format]
	pretty = format:%C(yellow)%ad %Cblue%h %Cgreen%<(7)%aN:%Cred%d %Creset%s
[log]
	date = format:%Y-%m-%d %H:%M:%S
[alias]
	l = log --date=short
	lr = log --date=relative
	q = log --all --decorate --oneline --graph
	cm = checkout master
[commit]
	gpgsign = false
[merge]
	ff = no
[delta]
	line-numbers = true
	hunk-header-style = omit
